generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id         String   @id @default(uuid())
  name       String
  price      Int //cents
  quantity   Int
  created_at DateTime @default(now())
  updated_at DateTime @default(now())

  @@map("products")
}
